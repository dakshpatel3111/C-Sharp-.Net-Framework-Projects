        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Midterm"
        mc:Ignorable="d"
        Title="About_Page" Height="450" Width="800">
    <Grid Background="#FF9AD8DE">
